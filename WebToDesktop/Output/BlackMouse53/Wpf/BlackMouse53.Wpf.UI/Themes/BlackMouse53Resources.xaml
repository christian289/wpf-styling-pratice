<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Container Size -->
    <!-- CSS: .container { width: 300px; height: 340px; } -->
    <sys:Double x:Key="BlackMouse53.Container.Width">300</sys:Double>
    <sys:Double x:Key="BlackMouse53.Container.Height">340</sys:Double>

    <!-- Bird Color -->
    <Color x:Key="BlackMouse53.Bird.Color">#FFFFFF</Color>
    <SolidColorBrush x:Key="BlackMouse53.Bird.Brush" Color="{StaticResource BlackMouse53.Bird.Color}" />

    <!-- Wing Bottom -->
    <!-- CSS: width: 120px; height: 120px; left: 92px; top: 94px; transform: rotate(86deg); -->
    <sys:Double x:Key="BlackMouse53.WingBottom.Width">120</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingBottom.Height">120</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingBottom.Left">92</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingBottom.Top">94</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingBottom.Angle">86</sys:Double>

    <!-- Wing Top -->
    <!-- CSS: width: 125px; height: 160px; left: 88px; top: 8px; transform: rotate(132deg); -->
    <sys:Double x:Key="BlackMouse53.WingTop.Width">125</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingTop.Height">160</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingTop.Left">88</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingTop.Top">8</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingTop.Angle">132</sys:Double>

    <!-- Wing Middle -->
    <!-- CSS: width: 130px; height: 130px; left: 73px; top: 44px; transform: rotate(7deg); -->
    <sys:Double x:Key="BlackMouse53.WingMiddle.Width">130</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingMiddle.Height">130</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingMiddle.Left">73</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingMiddle.Top">44</sys:Double>
    <sys:Double x:Key="BlackMouse53.WingMiddle.Angle">7</sys:Double>

    <!-- Beak Bottom -->
    <!-- CSS: width: 112px; height: 140px; left: 240px; top: -36px; transform: rotate(20deg); -->
    <sys:Double x:Key="BlackMouse53.BeakBottom.Width">112</sys:Double>
    <sys:Double x:Key="BlackMouse53.BeakBottom.Height">140</sys:Double>
    <sys:Double x:Key="BlackMouse53.BeakBottom.Left">240</sys:Double>
    <sys:Double x:Key="BlackMouse53.BeakBottom.Top">-36</sys:Double>
    <sys:Double x:Key="BlackMouse53.BeakBottom.Angle">20</sys:Double>

    <!-- Beak Top -->
    <!-- CSS: width: 112px; height: 112px; left: 225px; top: -33px; transform: rotate(15deg); -->
    <sys:Double x:Key="BlackMouse53.BeakTop.Width">112</sys:Double>
    <sys:Double x:Key="BlackMouse53.BeakTop.Height">112</sys:Double>
    <sys:Double x:Key="BlackMouse53.BeakTop.Left">225</sys:Double>
    <sys:Double x:Key="BlackMouse53.BeakTop.Top">-33</sys:Double>
    <sys:Double x:Key="BlackMouse53.BeakTop.Angle">15</sys:Double>

    <!-- Torso -->
    <!-- CSS: width: 200px; height: 200px; left: 117px; top: 8px; transform: rotate(10deg); -->
    <sys:Double x:Key="BlackMouse53.Torso.Width">200</sys:Double>
    <sys:Double x:Key="BlackMouse53.Torso.Height">200</sys:Double>
    <sys:Double x:Key="BlackMouse53.Torso.Left">117</sys:Double>
    <sys:Double x:Key="BlackMouse53.Torso.Top">8</sys:Double>
    <sys:Double x:Key="BlackMouse53.Torso.Angle">10</sys:Double>

    <!-- Tummy -->
    <!-- CSS: width: 320px; height: 320px; left: -3px; top: -55px; -->
    <sys:Double x:Key="BlackMouse53.Tummy.Width">320</sys:Double>
    <sys:Double x:Key="BlackMouse53.Tummy.Height">320</sys:Double>
    <sys:Double x:Key="BlackMouse53.Tummy.Left">-3</sys:Double>
    <sys:Double x:Key="BlackMouse53.Tummy.Top">-55</sys:Double>

    <!-- Tail -->
    <!-- CSS: width: 228px; height: 150px; left: 28px; top: 116px; transform: rotate(5deg); -->
    <sys:Double x:Key="BlackMouse53.Tail.Width">228</sys:Double>
    <sys:Double x:Key="BlackMouse53.Tail.Height">150</sys:Double>
    <sys:Double x:Key="BlackMouse53.Tail.Left">28</sys:Double>
    <sys:Double x:Key="BlackMouse53.Tail.Top">116</sys:Double>
    <sys:Double x:Key="BlackMouse53.Tail.Angle">5</sys:Double>

    <!-- Head -->
    <!-- CSS: width: 116px; height: 116px; left: 200px; top: 34px; -->
    <sys:Double x:Key="BlackMouse53.Head.Width">116</sys:Double>
    <sys:Double x:Key="BlackMouse53.Head.Height">116</sys:Double>
    <sys:Double x:Key="BlackMouse53.Head.Left">200</sys:Double>
    <sys:Double x:Key="BlackMouse53.Head.Top">34</sys:Double>

    <!-- Twitter Container Offset -->
    <!-- CSS: .twitter { transform: translate(-15%, 20%); } -->
    <!-- -15% of 300 = -45, 20% of 340 = 68 -->
    <sys:Double x:Key="BlackMouse53.Twitter.OffsetX">-45</sys:Double>
    <sys:Double x:Key="BlackMouse53.Twitter.OffsetY">68</sys:Double>

</ResourceDictionary>
