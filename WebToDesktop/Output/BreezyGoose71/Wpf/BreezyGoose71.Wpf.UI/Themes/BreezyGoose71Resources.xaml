<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Colors -->
    <!-- 배경색: teal -->
    <!-- Background color: teal -->
    <SolidColorBrush x:Key="BreezyGoose71.Background" Color="Teal" />

    <!-- 아이콘 테두리 색상: 흰색 -->
    <!-- Icon stroke color: white -->
    <SolidColorBrush x:Key="BreezyGoose71.Icon.Stroke" Color="White" />

    <!-- 아이콘 채우기 색상 (체크 전): 투명 -->
    <!-- Icon fill color (unchecked): transparent -->
    <SolidColorBrush x:Key="BreezyGoose71.Icon.Fill.Unchecked" Color="Transparent" />

    <!-- 아이콘 채우기 색상 (체크 후): 흰색 -->
    <!-- Icon fill color (checked): white -->
    <SolidColorBrush x:Key="BreezyGoose71.Icon.Fill.Checked" Color="White" />

    <!-- Sizes -->
    <!-- 컨트롤 너비: 45px -->
    <!-- Control width: 45px -->
    <sys:Double x:Key="BreezyGoose71.Width">45</sys:Double>

    <!-- 컨트롤 높이: 45px -->
    <!-- Control height: 45px -->
    <sys:Double x:Key="BreezyGoose71.Height">45</sys:Double>

    <!-- 모서리 반경: 10px -->
    <!-- Corner radius: 10px -->
    <CornerRadius x:Key="BreezyGoose71.CornerRadius">10</CornerRadius>

    <!-- 아이콘 너비: 15px -->
    <!-- Icon width: 15px -->
    <sys:Double x:Key="BreezyGoose71.Icon.Width">15</sys:Double>

    <!-- 아이콘 높이: 21px (비율 유지: 15 * 70/50) -->
    <!-- Icon height: 21px (aspect ratio: 15 * 70/50) -->
    <sys:Double x:Key="BreezyGoose71.Icon.Height">21</sys:Double>

    <!-- 테두리 두께: 2px (원본 7px를 스케일 조정) -->
    <!-- Stroke thickness: 2px (scaled from original 7px) -->
    <sys:Double x:Key="BreezyGoose71.Icon.StrokeThickness">2</sys:Double>

    <!-- Path Data -->
    <!-- 북마크 아이콘 경로 -->
    <!-- Bookmark icon path -->
    <Geometry x:Key="BreezyGoose71.Icon.Geometry">M46 62.0085L46 3.88139L3.99609 3.88139L3.99609 62.0085L24.5 45.5L46 62.0085Z</Geometry>

</ResourceDictionary>
