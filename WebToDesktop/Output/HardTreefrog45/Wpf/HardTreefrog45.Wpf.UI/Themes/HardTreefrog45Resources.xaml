<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Container -->
    <!-- max-w-[432px] -->
    <sys:Double x:Key="HardTreefrog45.Container.MaxWidth">432</sys:Double>
    <!-- bg-white -->
    <SolidColorBrush x:Key="HardTreefrog45.Container.Background" Color="#FFFFFF"/>
    <!-- rounded-md (6px) -->
    <CornerRadius x:Key="HardTreefrog45.Container.CornerRadius">6</CornerRadius>
    <!-- shadow-lg -->
    <sys:Double x:Key="HardTreefrog45.Container.ShadowBlurRadius">15</sys:Double>
    <sys:Double x:Key="HardTreefrog45.Container.ShadowOpacity">0.3</sys:Double>

    <!-- Header -->
    <!-- px-4 py-3 -->
    <Thickness x:Key="HardTreefrog45.Header.Padding">16,12,16,12</Thickness>
    <!-- font-bold, font-size: 32px -->
    <sys:Double x:Key="HardTreefrog45.Title.FontSize">32</sys:Double>
    <FontWeight x:Key="HardTreefrog45.Title.FontWeight">Bold</FontWeight>
    <!-- text-gray-500, font-size: 15px -->
    <sys:Double x:Key="HardTreefrog45.Subtitle.FontSize">15</sys:Double>
    <SolidColorBrush x:Key="HardTreefrog45.Subtitle.Foreground" Color="#6B7280"/>
    <!-- text-gray-600 -->
    <SolidColorBrush x:Key="HardTreefrog45.CloseButton.Foreground" Color="#4B5563"/>

    <!-- Separator -->
    <!-- bg-gray-600 -->
    <SolidColorBrush x:Key="HardTreefrog45.Separator.Background" Color="#4B5563"/>

    <!-- Form Content -->
    <!-- px-4 pt-3 pb-6 -->
    <Thickness x:Key="HardTreefrog45.Content.Padding">16,12,16,24</Thickness>
    <!-- space-y-3 -->
    <sys:Double x:Key="HardTreefrog45.Content.Spacing">12</sys:Double>

    <!-- Input Fields -->
    <!-- ring-1 ring-gray-400 -->
    <SolidColorBrush x:Key="HardTreefrog45.Input.BorderBrush" Color="#9CA3AF"/>
    <Thickness x:Key="HardTreefrog45.Input.BorderThickness">1</Thickness>
    <!-- rounded-md -->
    <CornerRadius x:Key="HardTreefrog45.Input.CornerRadius">6</CornerRadius>
    <!-- px-2 py-2 -->
    <Thickness x:Key="HardTreefrog45.Input.Padding">8,8,8,8</Thickness>
    <!-- bg-gray-100 -->
    <SolidColorBrush x:Key="HardTreefrog45.Input.Background" Color="#F3F4F6"/>
    <!-- focus:placeholder-gray-500 -->
    <SolidColorBrush x:Key="HardTreefrog45.Input.PlaceholderForeground" Color="#6B7280"/>

    <!-- Select Fields -->
    <!-- px-1 py-1.5 -->
    <Thickness x:Key="HardTreefrog45.Select.Padding">4,6,4,6</Thickness>

    <!-- Gender Radio Button Labels -->
    <!-- border border-gray-400, rounded-md, px-2 py-1 -->
    <Thickness x:Key="HardTreefrog45.GenderOption.Padding">8,4,8,4</Thickness>

    <!-- Date of Birth / Gender Label -->
    <!-- text-gray-500, font-size: 12px -->
    <sys:Double x:Key="HardTreefrog45.Label.FontSize">12</sys:Double>
    <SolidColorBrush x:Key="HardTreefrog45.Label.Foreground" Color="#6B7280"/>

    <!-- Info Text -->
    <!-- text-gray-600, font-size: 11px -->
    <sys:Double x:Key="HardTreefrog45.InfoText.FontSize">11</sys:Double>
    <SolidColorBrush x:Key="HardTreefrog45.InfoText.Foreground" Color="#4B5563"/>

    <!-- Link -->
    <!-- hover:text-blue-900 -->
    <SolidColorBrush x:Key="HardTreefrog45.Link.HoverForeground" Color="#1E3A8A"/>

    <!-- Sign Up Button -->
    <!-- background-color: #00A400 -->
    <SolidColorBrush x:Key="HardTreefrog45.SignUpButton.Background" Color="#00A400"/>
    <!-- text-white -->
    <SolidColorBrush x:Key="HardTreefrog45.SignUpButton.Foreground" Color="#FFFFFF"/>
    <!-- font-bold, font-size: 18px -->
    <sys:Double x:Key="HardTreefrog45.SignUpButton.FontSize">18</sys:Double>
    <FontWeight x:Key="HardTreefrog45.SignUpButton.FontWeight">Bold</FontWeight>
    <!-- px-16 py-1, rounded-md -->
    <Thickness x:Key="HardTreefrog45.SignUpButton.Padding">64,4,64,4</Thickness>

    <!-- Close Icon Path -->
    <!-- M6 18L18 6M6 6l12 12 -->
    <Geometry x:Key="HardTreefrog45.CloseIcon.Geometry">M6,18 L18,6 M6,6 L18,12</Geometry>

</ResourceDictionary>
