<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="using:BlackRabbit68.Avalonia.Lib.Controls">

    <!--
    BitcoinCoin 스타일 - 회전하는 비트코인 동전 애니메이션
    BitcoinCoin Style - Rotating Bitcoin coin animation

    원본 출처: Uiverse.io by JohnnyCSilva
    Original source: Uiverse.io by JohnnyCSilva

    CSS 3D rotateY를 AvaloniaUI 2D ScaleX 애니메이션으로 시뮬레이션
    CSS 3D rotateY simulated with AvaloniaUI 2D ScaleX animation
    -->

    <!-- 비트코인 오렌지 색상 / Bitcoin orange color -->
    <Color x:Key="BitcoinCoin.OrangeColor">#F7931A</Color>
    <SolidColorBrush x:Key="BitcoinCoin.OrangeBrush" Color="{StaticResource BitcoinCoin.OrangeColor}"/>

    <!-- 동전 가장자리 그라데이션 / Coin edge gradient -->
    <LinearGradientBrush x:Key="BitcoinCoin.EdgeGradient" StartPoint="0%,0%" EndPoint="0%,100%">
        <GradientStop Color="#FAA504" Offset="0"/>
        <GradientStop Color="#141001" Offset="1"/>
    </LinearGradientBrush>

    <ControlTheme x:Key="{x:Type controls:BitcoinCoin}" TargetType="controls:BitcoinCoin">
        <Setter Property="Width" Value="{Binding CoinSize, RelativeSource={RelativeSource Self}}"/>
        <Setter Property="Height" Value="{Binding CoinSize, RelativeSource={RelativeSource Self}}"/>
        <Setter Property="Template">
            <ControlTemplate TargetType="controls:BitcoinCoin">
                <Panel>
                    <!-- 동전 가장자리 (얇은 중앙 부분) / Coin edge (thin center part) -->
                    <Border Name="PART_Edge"
                            Width="20"
                            Height="{TemplateBinding Height}"
                            CornerRadius="10"
                            Background="{StaticResource BitcoinCoin.EdgeGradient}"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center">
                        <Border.RenderTransform>
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Border.RenderTransform>
                    </Border>

                    <!-- 동전 앞면 (비트코인 로고) / Coin front (Bitcoin logo) -->
                    <Ellipse Name="PART_Front"
                             Width="{TemplateBinding Width}"
                             Height="{TemplateBinding Height}"
                             Fill="{StaticResource BitcoinCoin.OrangeBrush}"
                             HorizontalAlignment="Center"
                             VerticalAlignment="Center">
                        <Ellipse.RenderTransform>
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Ellipse.RenderTransform>
                    </Ellipse>

                    <!-- 비트코인 로고 SVG / Bitcoin logo SVG -->
                    <Viewbox Name="PART_FrontLogo"
                             Width="{TemplateBinding Width}"
                             Height="{TemplateBinding Height}"
                             HorizontalAlignment="Center"
                             VerticalAlignment="Center">
                        <Viewbox.RenderTransform>
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Viewbox.RenderTransform>
                        <Canvas Width="4091.27" Height="4091.73">
                            <!-- 배경 원 / Background circle -->
                            <Path Fill="#F7931A">
                                <Path.Data>
                                    <PathGeometry Figures="M4030.06 2540.77c-273.24,1096.01 -1383.32,1763.02 -2479.46,1489.71 -1095.68,-273.24 -1762.69,-1383.39 -1489.33,-2479.31 273.12,-1096.13 1383.2,-1763.19 2479,-1489.95 1096.06,273.24 1763.03,1383.51 1489.76,2479.57l0.02 -0.02z"/>
                                </Path.Data>
                            </Path>
                            <!-- 비트코인 심볼 / Bitcoin symbol -->
                            <Path Fill="White">
                                <Path.Data>
                                    <PathGeometry Figures="M2947.77 1754.38c40.72,-272.26 -166.56,-418.61 -450,-516.24l91.95 -368.8 -224.5 -55.94 -89.51 359.09c-59.02,-14.72 -119.63,-28.59 -179.87,-42.34l90.16 -361.46 -224.36 -55.94 -92 368.68c-48.84,-11.12 -96.81,-22.11 -143.35,-33.69l0.26 -1.16 -309.59 -77.31 -59.72 239.78c0,0 166.56,38.18 163.05,40.53 90.91,22.69 107.35,82.87 104.62,130.57l-104.74 420.15c6.26,1.59 14.38,3.89 23.34,7.49 -7.49,-1.86 -15.46,-3.89 -23.73,-5.87l-146.81 588.57c-11.11,27.62 -39.31,69.07 -102.87,53.33 2.25,3.26 -163.17,-40.72 -163.17,-40.72l-111.46 256.98 292.15 72.83c54.35,13.63 107.61,27.89 160.06,41.3l-92.9 373.03 224.24 55.94 92 -369.07c61.26,16.63 120.71,31.97 178.91,46.43l-91.69 367.33 224.51 55.94 92.89 -372.33c382.82,72.45 670.67,43.24 791.83,-303.02 97.63,-278.78 -4.86,-439.58 -206.26,-544.44 146.69,-33.83 257.18,-130.31 286.64,-329.61l-0.07 -0.05zm-512.93 719.26c-69.38,278.78 -538.76,128.08 -690.94,90.29l123.28 -494.2c152.17,37.99 640.17,113.17 567.67,403.91zm69.43 -723.3c-63.29,253.58 -453.96,124.75 -580.69,93.16l111.77 -448.21c126.73,31.59 534.85,90.55 468.94,355.05l-0.02 0z"/>
                                </Path.Data>
                            </Path>
                        </Canvas>
                    </Viewbox>

                    <!-- 동전 뒷면 / Coin back -->
                    <Ellipse Name="PART_Back"
                             Width="{TemplateBinding Width}"
                             Height="{TemplateBinding Height}"
                             Fill="{StaticResource BitcoinCoin.OrangeBrush}"
                             HorizontalAlignment="Center"
                             VerticalAlignment="Center"
                             Opacity="0">
                        <Ellipse.RenderTransform>
                            <ScaleTransform ScaleX="-1" ScaleY="1"/>
                        </Ellipse.RenderTransform>
                    </Ellipse>

                    <!-- 뒷면 로고 (좌우 반전) / Back logo (horizontally flipped) -->
                    <Viewbox Name="PART_BackLogo"
                             Width="{TemplateBinding Width}"
                             Height="{TemplateBinding Height}"
                             HorizontalAlignment="Center"
                             VerticalAlignment="Center"
                             Opacity="0">
                        <Viewbox.RenderTransform>
                            <ScaleTransform ScaleX="-1" ScaleY="1"/>
                        </Viewbox.RenderTransform>
                        <Canvas Width="4091.27" Height="4091.73">
                            <Path Fill="#F7931A">
                                <Path.Data>
                                    <PathGeometry Figures="M4030.06 2540.77c-273.24,1096.01 -1383.32,1763.02 -2479.46,1489.71 -1095.68,-273.24 -1762.69,-1383.39 -1489.33,-2479.31 273.12,-1096.13 1383.2,-1763.19 2479,-1489.95 1096.06,273.24 1763.03,1383.51 1489.76,2479.57l0.02 -0.02z"/>
                                </Path.Data>
                            </Path>
                            <Path Fill="White">
                                <Path.Data>
                                    <PathGeometry Figures="M2947.77 1754.38c40.72,-272.26 -166.56,-418.61 -450,-516.24l91.95 -368.8 -224.5 -55.94 -89.51 359.09c-59.02,-14.72 -119.63,-28.59 -179.87,-42.34l90.16 -361.46 -224.36 -55.94 -92 368.68c-48.84,-11.12 -96.81,-22.11 -143.35,-33.69l0.26 -1.16 -309.59 -77.31 -59.72 239.78c0,0 166.56,38.18 163.05,40.53 90.91,22.69 107.35,82.87 104.62,130.57l-104.74 420.15c6.26,1.59 14.38,3.89 23.34,7.49 -7.49,-1.86 -15.46,-3.89 -23.73,-5.87l-146.81 588.57c-11.11,27.62 -39.31,69.07 -102.87,53.33 2.25,3.26 -163.17,-40.72 -163.17,-40.72l-111.46 256.98 292.15 72.83c54.35,13.63 107.61,27.89 160.06,41.3l-92.9 373.03 224.24 55.94 92 -369.07c61.26,16.63 120.71,31.97 178.91,46.43l-91.69 367.33 224.51 55.94 92.89 -372.33c382.82,72.45 670.67,43.24 791.83,-303.02 97.63,-278.78 -4.86,-439.58 -206.26,-544.44 146.69,-33.83 257.18,-130.31 286.64,-329.61l-0.07 -0.05zm-512.93 719.26c-69.38,278.78 -538.76,128.08 -690.94,90.29l123.28 -494.2c152.17,37.99 640.17,113.17 567.67,403.91zm69.43 -723.3c-63.29,253.58 -453.96,124.75 -580.69,93.16l111.77 -448.21c126.73,31.59 534.85,90.55 468.94,355.05l-0.02 0z"/>
                                </Path.Data>
                            </Path>
                        </Canvas>
                    </Viewbox>
                </Panel>
            </ControlTemplate>
        </Setter>

        <!-- 회전 애니메이션 / Rotation animation -->
        <Style Selector="^">
            <Style.Animations>
                <!-- 앞면 ScaleX: 1 → 0 → -1 → 0 → 1 (7초) -->
                <!-- Front ScaleX: 1 → 0 → -1 → 0 → 1 (7s) -->
                <Animation Duration="0:0:7" IterationCount="Infinite" PlaybackDirection="Normal">
                    <KeyFrame Cue="0%">
                        <Setter Property="Opacity" Value="1"/>
                    </KeyFrame>
                    <KeyFrame Cue="100%">
                        <Setter Property="Opacity" Value="1"/>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

        <Style Selector="^ /template/ Ellipse#PART_Front">
            <Style.Animations>
                <Animation Duration="0:0:7" IterationCount="Infinite" PlaybackDirection="Normal" Easing="LinearEasing">
                    <KeyFrame Cue="0%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="1"/>
                    </KeyFrame>
                    <KeyFrame Cue="25%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="50%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="75%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="100%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="1"/>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

        <Style Selector="^ /template/ Viewbox#PART_FrontLogo">
            <Style.Animations>
                <Animation Duration="0:0:7" IterationCount="Infinite" PlaybackDirection="Normal" Easing="LinearEasing">
                    <KeyFrame Cue="0%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="1"/>
                    </KeyFrame>
                    <KeyFrame Cue="25%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="50%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="75%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="100%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="1"/>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

        <Style Selector="^ /template/ Ellipse#PART_Back">
            <Style.Animations>
                <Animation Duration="0:0:7" IterationCount="Infinite" PlaybackDirection="Normal" Easing="LinearEasing">
                    <KeyFrame Cue="0%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="25%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="-1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="1"/>
                    </KeyFrame>
                    <KeyFrame Cue="50%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="75%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="100%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

        <Style Selector="^ /template/ Viewbox#PART_BackLogo">
            <Style.Animations>
                <Animation Duration="0:0:7" IterationCount="Infinite" PlaybackDirection="Normal" Easing="LinearEasing">
                    <KeyFrame Cue="0%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="25%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="-1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="1"/>
                    </KeyFrame>
                    <KeyFrame Cue="50%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="75%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="100%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="0" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

        <Style Selector="^ /template/ Border#PART_Edge">
            <Style.Animations>
                <Animation Duration="0:0:7" IterationCount="Infinite" PlaybackDirection="Normal" Easing="LinearEasing">
                    <KeyFrame Cue="0%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="24%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="25%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="1"/>
                    </KeyFrame>
                    <KeyFrame Cue="26%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="49%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="50%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="1"/>
                    </KeyFrame>
                    <KeyFrame Cue="51%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="74%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="75%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="1"/>
                    </KeyFrame>
                    <KeyFrame Cue="76%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="99%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                    <KeyFrame Cue="100%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                        </Setter>
                        <Setter Property="Opacity" Value="0"/>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

    </ControlTheme>

</ResourceDictionary>
