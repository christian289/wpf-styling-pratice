<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Size Values -->
    <!-- [height] 3rem = 48px -->
    <sys:Double x:Key="TidyVampirebat71.Size">48</sys:Double>
    <!-- [bar-width] 0.25rem = 4px -->
    <sys:Double x:Key="TidyVampirebat71.Bar.Width">4</sys:Double>
    <!-- [bar-height] 3rem = 48px -->
    <sys:Double x:Key="TidyVampirebat71.Bar.Height">48</sys:Double>
    <!-- [cover-size] 4rem = 64px -->
    <sys:Double x:Key="TidyVampirebat71.Cover.Size">64</sys:Double>
    <!-- [cover2-size] 2rem = 32px -->
    <sys:Double x:Key="TidyVampirebat71.Cover2.Size">32</sys:Double>
    <!-- [nut-size] 0.3rem = 4.8px -->
    <sys:Double x:Key="TidyVampirebat71.Nut.Size">5</sys:Double>
    <!-- [lock-size] 1.5rem = 24px -->
    <sys:Double x:Key="TidyVampirebat71.Lock.Size">24</sys:Double>
    <!-- [outline-width] 0.25rem = 4px -->
    <sys:Double x:Key="TidyVampirebat71.Cover2.OutlineWidth">4</sys:Double>

    <!-- Animation Timing -->
    <!-- [rotate-offset] 45deg -->
    <sys:Double x:Key="TidyVampirebat71.RotateOffset">45</sys:Double>
    <!-- [time-offset] 200ms -->
    <Duration x:Key="TidyVampirebat71.TimeOffset">0:0:0.2</Duration>
    <!-- [delay] 250ms -->
    <sys:Double x:Key="TidyVampirebat71.Delay">0.25</sys:Double>
    <!-- [total-duration] time-offset + delay * 7 = 200 + 1750 = 1950ms -->
    <Duration x:Key="TidyVampirebat71.TotalDuration">0:0:1.95</Duration>

    <!-- Colors - Default (Unchecked) -->
    <SolidColorBrush x:Key="TidyVampirebat71.Bar.Background" Color="White"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Nut.Background" Color="White"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Cover.Background" Color="#2A3439"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Cover2.Background" Color="#91A3B0"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Cover2.Outline" Color="#B2FFFF"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Lock.Stroke" Color="#2A3439"/>

    <!-- Rainbow Colors (Checked state) -->
    <SolidColorBrush x:Key="TidyVampirebat71.Rainbow.Color1" Color="#FFADAD"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Rainbow.Color2" Color="#FFD6A5"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Rainbow.Color3" Color="#FDFFB6"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Rainbow.Color4" Color="#CAFFBF"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Rainbow.Color5" Color="#9BF6FF"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Rainbow.Color6" Color="#A0C4FF"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Rainbow.Color7" Color="#BDB2FF"/>
    <SolidColorBrush x:Key="TidyVampirebat71.Rainbow.Color8" Color="#FFC6FF"/>

