<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Color Resources -->
    <Color x:Key="WetQuail86.Light.Color">#FFFFFF</Color>
    <Color x:Key="WetQuail86.Dark.Color">#323232</Color>

    <SolidColorBrush x:Key="WetQuail86.Light.Brush" Color="{StaticResource WetQuail86.Light.Color}"/>
    <SolidColorBrush x:Key="WetQuail86.Dark.Brush" Color="{StaticResource WetQuail86.Dark.Color}"/>

    <!-- Size Resources -->
    <x:Double x:Key="WetQuail86.CornerRadius">8</x:Double>
    <CornerRadius x:Key="WetQuail86.CornerRadiusValue">8</CornerRadius>
    <Thickness x:Key="WetQuail86.Padding">8</Thickness>
    <x:Double x:Key="WetQuail86.MaxWidth">200</x:Double>
    <x:Double x:Key="WetQuail86.TitleFontSize">15</x:Double>

</ResourceDictionary>
