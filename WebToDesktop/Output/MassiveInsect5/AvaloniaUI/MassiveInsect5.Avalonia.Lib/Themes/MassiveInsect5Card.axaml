                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="using:MassiveInsect5.Avalonia.Lib.Controls">

    <!-- Color Resources -->
    <Color x:Key="Card.Background">#D3D3D3</Color>
    <Color x:Key="Card.Shadow">#33646C6F</Color>

    <!-- Background Gradient Colors -->
    <Color x:Key="Background.Gradient1">#FF89CC</Color>
    <Color x:Key="Background.Gradient2">#9CB8EC</Color>
    <Color x:Key="Background.Gradient3">#00FFEE</Color>
    <Color x:Key="Background.Gradient4">#62C2FE</Color>

    <!-- Box1 (Instagram) Gradient Colors -->
    <Color x:Key="Box1.Gradient1">#FDF497</Color>
    <Color x:Key="Box1.Gradient2">#FF53D4</Color>
    <Color x:Key="Box1.Gradient3">#62C2FE</Color>

    <!-- Box2 (Twitter) Gradient Colors -->
    <Color x:Key="Box2.Gradient1">#91E9FF</Color>
    <Color x:Key="Box2.Gradient2">#00ACEE</Color>

    <!-- Box3 (Discord) Gradient Colors -->
    <Color x:Key="Box3.Gradient1">#969FFF</Color>
    <Color x:Key="Box3.Gradient2">#B349FF</Color>

    <!-- Box Semi-transparent Background -->
    <Color x:Key="Box.Background">#63FFFFFF</Color>

    <!-- Icon Fill Color -->
    <Color x:Key="Icon.Fill">#CBFFFFFF</Color>

