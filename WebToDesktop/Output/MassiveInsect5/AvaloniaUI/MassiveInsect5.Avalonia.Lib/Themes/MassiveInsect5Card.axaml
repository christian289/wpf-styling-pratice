    <!-- MassiveInsect5Card Style -->
    <ControlTheme x:Key="{x:Type controls:MassiveInsect5Card}" TargetType="controls:MassiveInsect5Card">
        <Setter Property="Width" Value="200"/>
        <Setter Property="Height" Value="200"/>
        <Setter Property="ClipToBounds" Value="True"/>
        <Setter Property="Template">
            <ControlTemplate>
                <Border x:Name="PART_Root"
                        Width="{TemplateBinding Width}"
                        Height="{TemplateBinding Height}"
                        Background="{StaticResource Card.Background}"
                        CornerRadius="30"
                        ClipToBounds="True"
                        BoxShadow="0 7 29 0 #33646C6F"
                        RenderTransformOrigin="0.5,0.5">
                    <Border.RenderTransform>
                        <ScaleTransform ScaleX="1" ScaleY="1"/>
                    </Border.RenderTransform>

                    <Panel ClipToBounds="True">
                        <!-- Background Gradient -->
                        <Border x:Name="PART_Background">
                            <Border.Background>
                                <!-- RadialGradientBrush: GradientOrigin과 Center를 동일하게 설정 (AvaloniaUI Issue #19888) -->
                                <!-- radial-gradient(circle at 100% 107%, ...) -->
                                <RadialGradientBrush GradientOrigin="100%,107%" Center="100%,107%" RadiusX="1" RadiusY="1">
                                    <GradientStop Color="{StaticResource Background.Gradient1}" Offset="0"/>
                                    <GradientStop Color="{StaticResource Background.Gradient2}" Offset="0.3"/>
                                    <GradientStop Color="{StaticResource Background.Gradient3}" Offset="0.6"/>
                                    <GradientStop Color="{StaticResource Background.Gradient4}" Offset="1"/>
                                </RadialGradientBrush>
                            </Border.Background>
                        </Border>

                        <!-- Logo (UI letters) -->
                        <Canvas x:Name="PART_Logo"
                                Width="30" Height="32"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                RenderTransformOrigin="0.5,0.5">
                            <Canvas.RenderTransform>
                                <TranslateTransform X="0" Y="0"/>
                            </Canvas.RenderTransform>
                            <Path Data="{StaticResource Logo.Path1}" Fill="White"/>
                            <Path Data="{StaticResource Logo.Path2}" Fill="White"/>
                            <Path Data="{StaticResource Logo.Path3}" Fill="White"/>
                        </Canvas>

                        <!-- Box1 - Instagram (largest) -->
                        <Border x:Name="PART_Box1"
                                Width="140" Height="140"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Padding="10"
                                Background="{StaticResource Box.Background}"
                                BorderBrush="White"
                                BorderThickness="1,2,0,0"
                                CornerRadius="14,18,84,0"
                                BoxShadow="-7 7 29 0 #5D646C6F"
                                RenderTransformOrigin="0,1">
                            <Border.RenderTransform>
                                <TranslateTransform X="-140" Y="140"/>
                            </Border.RenderTransform>

                            <Panel>
                                <!-- Hover Gradient Overlay -->
                                <Border x:Name="PART_Box1Overlay"
                                        CornerRadius="14,18,84,0"
                                        Opacity="0">
                                    <Border.Background>
                                        <!-- radial-gradient(circle at 30% 107%, ...) -->
                                        <RadialGradientBrush GradientOrigin="30%,107%" Center="30%,107%" RadiusX="1" RadiusY="1">
                                            <GradientStop Color="{StaticResource Box1.Gradient1}" Offset="0"/>
                                            <GradientStop Color="{StaticResource Box1.Gradient1}" Offset="0.05"/>
                                            <GradientStop Color="{StaticResource Box1.Gradient2}" Offset="0.6"/>
                                            <GradientStop Color="{StaticResource Box1.Gradient3}" Offset="0.9"/>
                                        </RadialGradientBrush>
                                    </Border.Background>
                                </Border>

                                <!-- Instagram Icon -->
                                <Path x:Name="PART_Icon1"
                                      Data="{StaticResource Instagram.Path}"
                                      Fill="{StaticResource Icon.Fill}"
                                      Width="20" Height="20"
                                      Stretch="Uniform"
                                      HorizontalAlignment="Right"
                                      VerticalAlignment="Top"/>
                            </Panel>
                        </Border>

                        <!-- Box2 - Twitter (medium) -->
                        <Border x:Name="PART_Box2"
                                Width="100" Height="100"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Padding="10"
                                Background="{StaticResource Box.Background}"
                                BorderBrush="White"
                                BorderThickness="1,2,0,0"
                                CornerRadius="10,13,75,0"
                                BoxShadow="-7 7 29 0 #5D646C6F"
                                RenderTransformOrigin="0,1">
                            <Border.RenderTransform>
                                <TranslateTransform X="-100" Y="100"/>
                            </Border.RenderTransform>

                            <Panel>
                                <!-- Hover Gradient Overlay -->
                                <Border x:Name="PART_Box2Overlay"
                                        CornerRadius="10,13,75,0"
                                        Opacity="0">
                                    <Border.Background>
                                        <!-- radial-gradient(circle at 30% 107%, ...) -->
                                        <RadialGradientBrush GradientOrigin="30%,107%" Center="30%,107%" RadiusX="1" RadiusY="1">
                                            <GradientStop Color="{StaticResource Box2.Gradient1}" Offset="0"/>
                                            <GradientStop Color="{StaticResource Box2.Gradient2}" Offset="0.9"/>
                                        </RadialGradientBrush>
                                    </Border.Background>
                                </Border>

                                <!-- Twitter Icon -->
                                <Path x:Name="PART_Icon2"
                                      Data="{StaticResource Twitter.Path}"
                                      Fill="{StaticResource Icon.Fill}"
                                      Width="20" Height="20"
                                      Stretch="Uniform"
                                      HorizontalAlignment="Right"
                                      VerticalAlignment="Top"/>
                            </Panel>
                        </Border>

                        <!-- Box3 - Discord (small) -->
                        <Border x:Name="PART_Box3"
                                Width="60" Height="60"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Padding="10"
                                Background="{StaticResource Box.Background}"
                                BorderBrush="White"
                                BorderThickness="1,2,0,0"
                                CornerRadius="6,8,45,0"
                                BoxShadow="-7 7 29 0 #5D646C6F"
                                RenderTransformOrigin="0,1">
                            <Border.RenderTransform>
                                <TranslateTransform X="-60" Y="60"/>
                            </Border.RenderTransform>

                            <Panel>
                                <!-- Hover Gradient Overlay -->
                                <Border x:Name="PART_Box3Overlay"
                                        CornerRadius="6,8,45,0"
                                        Opacity="0">
                                    <Border.Background>
                                        <!-- radial-gradient(circle at 30% 107%, ...) -->
                                        <RadialGradientBrush GradientOrigin="30%,107%" Center="30%,107%" RadiusX="1" RadiusY="1">
                                            <GradientStop Color="{StaticResource Box3.Gradient1}" Offset="0"/>
                                            <GradientStop Color="{StaticResource Box3.Gradient2}" Offset="0.9"/>
                                        </RadialGradientBrush>
                                    </Border.Background>
                                </Border>

                                <!-- Discord Icon -->
                                <Path x:Name="PART_Icon3"
                                      Data="{StaticResource Discord.Path}"
                                      Fill="{StaticResource Icon.Fill}"
                                      Width="20" Height="20"
                                      Stretch="Uniform"
                                      HorizontalAlignment="Right"
                                      VerticalAlignment="Top"/>
                            </Panel>
                        </Border>

                        <!-- Box4 - (smallest, no icon) -->
                        <Border x:Name="PART_Box4"
                                Width="20" Height="20"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Background="{StaticResource Box.Background}"
                                BorderBrush="White"
                                BorderThickness="1,2,0,0"
                                CornerRadius="2,3,15,0"
                                BoxShadow="-7 7 29 0 #5D646C6F"
                                RenderTransformOrigin="0,1">
                            <Border.RenderTransform>
                                <TranslateTransform X="-20" Y="20"/>
                            </Border.RenderTransform>
                        </Border>
                    </Panel>
                </Border>
            </ControlTemplate>
        </Setter>

        <!-- PointerOver State - Card Scale -->
        <Style Selector="^:pointerover /template/ Border#PART_Root">
            <Style.Animations>
                <Animation Duration="0:0:1" Easing="CubicEaseInOut" FillMode="Forward">
                    <KeyFrame Cue="100%">
                        <Setter Property="RenderTransform">
                            <ScaleTransform ScaleX="1.1" ScaleY="1.1"/>
                        </Setter>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

        <!-- PointerOver State - Logo moves to bottom-right -->
        <Style Selector="^:pointerover /template/ Canvas#PART_Logo">
            <Style.Animations>
                <Animation Duration="0:0:0.6" Easing="CubicEaseInOut" FillMode="Forward">
                    <KeyFrame Cue="100%">
                        <Setter Property="HorizontalAlignment" Value="Right"/>
                        <Setter Property="VerticalAlignment" Value="Bottom"/>
                        <Setter Property="Margin" Value="0,0,20,20"/>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

        <!-- PointerOver State - Box1 slides in -->
        <Style Selector="^:pointerover /template/ Border#PART_Box1">
            <Style.Animations>
                <Animation Duration="0:0:1" Easing="CubicEaseInOut" FillMode="Forward">
                    <KeyFrame Cue="100%">
                        <Setter Property="RenderTransform">
                            <TranslateTransform X="-1" Y="1"/>
                        </Setter>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

        <!-- PointerOver State - Box2 slides in (delayed) -->
        <Style Selector="^:pointerover /template/ Border#PART_Box2">
            <Style.Animations>
                <Animation Duration="0:0:1" Delay="0:0:0.2" Easing="CubicEaseInOut" FillMode="Forward">
                    <KeyFrame Cue="100%">
                        <Setter Property="RenderTransform">
                            <TranslateTransform X="-1" Y="1"/>
                        </Setter>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

        <!-- PointerOver State - Box3 slides in (delayed) -->
        <Style Selector="^:pointerover /template/ Border#PART_Box3">
            <Style.Animations>
                <Animation Duration="0:0:1" Delay="0:0:0.4" Easing="CubicEaseInOut" FillMode="Forward">
                    <KeyFrame Cue="100%">
                        <Setter Property="RenderTransform">
                            <TranslateTransform X="-1" Y="1"/>
                        </Setter>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

        <!-- PointerOver State - Box4 slides in (delayed) -->
        <Style Selector="^:pointerover /template/ Border#PART_Box4">
            <Style.Animations>
                <Animation Duration="0:0:1" Delay="0:0:0.6" Easing="CubicEaseInOut" FillMode="Forward">
                    <KeyFrame Cue="100%">
                        <Setter Property="RenderTransform">
                            <TranslateTransform X="-1" Y="1"/>
                        </Setter>
                    </KeyFrame>
                </Animation>
            </Style.Animations>
        </Style>

        <!-- Box1 Hover - Show gradient overlay -->
        <Style Selector="^:pointerover /template/ Border#PART_Box1:pointerover /template/ Border#PART_Box1Overlay">
            <Setter Property="Opacity" Value="1"/>
        </Style>

        <!-- Box2 Hover - Show gradient overlay -->
        <Style Selector="^:pointerover /template/ Border#PART_Box2:pointerover /template/ Border#PART_Box2Overlay">
            <Setter Property="Opacity" Value="1"/>
        </Style>

        <!-- Box3 Hover - Show gradient overlay -->
        <Style Selector="^:pointerover /template/ Border#PART_Box3:pointerover /template/ Border#PART_Box3Overlay">
            <Setter Property="Opacity" Value="1"/>
        </Style>
    </ControlTheme>

</ResourceDictionary>
