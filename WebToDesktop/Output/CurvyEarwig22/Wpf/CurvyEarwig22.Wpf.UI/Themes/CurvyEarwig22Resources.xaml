<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Sizes -->
    <sys:Double x:Key="CurvyEarwig22.Input.Width">301</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.Input.Height">56</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.Input.FontSize">18</sys:Double>
    <CornerRadius x:Key="CurvyEarwig22.Input.CornerRadius">10</CornerRadius>
    <Thickness x:Key="CurvyEarwig22.Input.Padding">59,0,59,0</Thickness>

    <!-- Filter Icon Sizes -->
    <sys:Double x:Key="CurvyEarwig22.FilterIcon.Width">38</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.FilterIcon.Height">40</sys:Double>
    <CornerRadius x:Key="CurvyEarwig22.FilterIcon.CornerRadius">10</CornerRadius>
    <Thickness x:Key="CurvyEarwig22.FilterIcon.Margin">0,8,8,0</Thickness>

    <!-- Filter Border Sizes -->
    <sys:Double x:Key="CurvyEarwig22.FilterBorder.Width">40</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.FilterBorder.Height">42</sys:Double>
    <Thickness x:Key="CurvyEarwig22.FilterBorder.Margin">0,7,7,0</Thickness>

    <!-- Glow Layer Sizes -->
    <sys:Double x:Key="CurvyEarwig22.Glow.MaxWidth">354</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.Glow.MaxHeight">130</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.Glow.BlurRadius">30</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.Glow.Opacity">0.4</sys:Double>

    <!-- White Layer Sizes -->
    <sys:Double x:Key="CurvyEarwig22.White.MaxWidth">307</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.White.MaxHeight">63</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.White.BlurRadius">2</sys:Double>

    <!-- Border Layer Sizes -->
    <sys:Double x:Key="CurvyEarwig22.Border.MaxWidth">303</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.Border.MaxHeight">59</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.Border.BlurRadius">0.5</sys:Double>
    <CornerRadius x:Key="CurvyEarwig22.Border.CornerRadius">11</CornerRadius>

    <!-- DarkBorderBg Layer Sizes -->
    <sys:Double x:Key="CurvyEarwig22.DarkBorderBg.MaxWidth">312</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.DarkBorderBg.MaxHeight">65</sys:Double>
    <CornerRadius x:Key="CurvyEarwig22.DarkBorderBg.CornerRadius">12</CornerRadius>

    <!-- Common Layer Properties -->
    <sys:Double x:Key="CurvyEarwig22.Layer.BlurRadius">3</sys:Double>
    <CornerRadius x:Key="CurvyEarwig22.Layer.CornerRadius">12</CornerRadius>

    <!-- Colors -->
    <Color x:Key="CurvyEarwig22.Input.Background">#010201</Color>
    <Color x:Key="CurvyEarwig22.Input.Foreground">#FFFFFF</Color>
    <Color x:Key="CurvyEarwig22.Input.Placeholder">#C0B9C0</Color>

    <!-- Gradient Colors -->
    <Color x:Key="CurvyEarwig22.Gradient.Purple">#402FB5</Color>
    <Color x:Key="CurvyEarwig22.Gradient.Pink">#CF30AA</Color>
    <Color x:Key="CurvyEarwig22.Gradient.DarkPurple">#18116A</Color>
    <Color x:Key="CurvyEarwig22.Gradient.DarkPink">#6E1B60</Color>
    <Color x:Key="CurvyEarwig22.Gradient.LightPurple">#A099D8</Color>
    <Color x:Key="CurvyEarwig22.Gradient.LightPink">#DFA2DA</Color>
    <Color x:Key="CurvyEarwig22.Gradient.Dark">#1C191C</Color>
    <Color x:Key="CurvyEarwig22.Gradient.Black">#000000</Color>
    <Color x:Key="CurvyEarwig22.Gradient.Transparent">#00000000</Color>

    <!-- Filter Icon Background Gradient -->
    <Color x:Key="CurvyEarwig22.FilterIcon.TopColor">#161329</Color>
    <Color x:Key="CurvyEarwig22.FilterIcon.MiddleColor">#000000</Color>
    <Color x:Key="CurvyEarwig22.FilterIcon.BottomColor">#1D1B4B</Color>

    <!-- Filter Border Gradient Color -->
    <Color x:Key="CurvyEarwig22.FilterBorder.GradientColor">#3D3A4F</Color>

    <!-- Search Icon Gradient Colors -->
    <Color x:Key="CurvyEarwig22.SearchIcon.CircleStart">#F8E7F8</Color>
    <Color x:Key="CurvyEarwig22.SearchIcon.CircleEnd">#B6A9B7</Color>
    <Color x:Key="CurvyEarwig22.SearchIcon.LineStart">#B6A9B7</Color>
    <Color x:Key="CurvyEarwig22.SearchIcon.LineEnd">#837484</Color>

    <!-- Filter Icon Stroke Color -->
    <Color x:Key="CurvyEarwig22.FilterIcon.StrokeColor">#D6D6E6</Color>

    <!-- Pink Mask -->
    <Color x:Key="CurvyEarwig22.PinkMask.Color">#CF30AA</Color>
    <sys:Double x:Key="CurvyEarwig22.PinkMask.Width">30</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.PinkMask.Height">20</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.PinkMask.BlurRadius">20</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.PinkMask.Opacity">0.8</sys:Double>
    <Thickness x:Key="CurvyEarwig22.PinkMask.Margin">5,10,0,0</Thickness>

    <!-- Input Mask -->
    <sys:Double x:Key="CurvyEarwig22.InputMask.Width">100</sys:Double>
    <sys:Double x:Key="CurvyEarwig22.InputMask.Height">20</sys:Double>
    <Thickness x:Key="CurvyEarwig22.InputMask.Margin">70,18,0,0</Thickness>

    <!-- Search Icon Position -->
    <Thickness x:Key="CurvyEarwig22.SearchIcon.Margin">20,15,0,0</Thickness>
    <sys:Double x:Key="CurvyEarwig22.SearchIcon.Size">24</sys:Double>

    <!-- Brushes -->
    <SolidColorBrush x:Key="CurvyEarwig22.Input.BackgroundBrush" Color="{StaticResource CurvyEarwig22.Input.Background}"/>
    <SolidColorBrush x:Key="CurvyEarwig22.Input.ForegroundBrush" Color="{StaticResource CurvyEarwig22.Input.Foreground}"/>
    <SolidColorBrush x:Key="CurvyEarwig22.Input.PlaceholderBrush" Color="{StaticResource CurvyEarwig22.Input.Placeholder}"/>
    <SolidColorBrush x:Key="CurvyEarwig22.PinkMask.Brush" Color="{StaticResource CurvyEarwig22.PinkMask.Color}"/>
    <SolidColorBrush x:Key="CurvyEarwig22.FilterIcon.StrokeBrush" Color="{StaticResource CurvyEarwig22.FilterIcon.StrokeColor}"/>

    <!-- Filter Icon Background -->
    <LinearGradientBrush x:Key="CurvyEarwig22.FilterIcon.BackgroundBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="{StaticResource CurvyEarwig22.FilterIcon.TopColor}" Offset="0"/>
        <GradientStop Color="{StaticResource CurvyEarwig22.FilterIcon.MiddleColor}" Offset="0.5"/>
        <GradientStop Color="{StaticResource CurvyEarwig22.FilterIcon.BottomColor}" Offset="1"/>
    </LinearGradientBrush>

    <!-- Input Mask Gradient -->
    <LinearGradientBrush x:Key="CurvyEarwig22.InputMask.BackgroundBrush" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Color="Transparent" Offset="0"/>
        <GradientStop Color="Black" Offset="1"/>
    </LinearGradientBrush>

    <!-- Search Icon Circle Gradient -->
    <LinearGradientBrush x:Key="CurvyEarwig22.SearchIcon.CircleBrush" StartPoint="0,0" EndPoint="1,1">
        <LinearGradientBrush.Transform>
            <RotateTransform Angle="50" CenterX="0.5" CenterY="0.5"/>
        </LinearGradientBrush.Transform>
        <GradientStop Color="{StaticResource CurvyEarwig22.SearchIcon.CircleStart}" Offset="0"/>
        <GradientStop Color="{StaticResource CurvyEarwig22.SearchIcon.CircleEnd}" Offset="0.5"/>
    </LinearGradientBrush>

    <!-- Search Icon Line Gradient -->
    <LinearGradientBrush x:Key="CurvyEarwig22.SearchIcon.LineBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="{StaticResource CurvyEarwig22.SearchIcon.LineStart}" Offset="0"/>
        <GradientStop Color="{StaticResource CurvyEarwig22.SearchIcon.LineEnd}" Offset="0.5"/>
    </LinearGradientBrush>

